    //玩家开宝箱动作
    "treasure_box_open_datadriven"
    {
        "BaseClass"				"ability_datadriven"
		"AbilityBehavior"       "DOTA_ABILITY_BEHAVIOR_PASSIVE | DOTA_ABILITY_BEHAVIOR_NO_TARGET| DOTA_ABILITY_BEHAVIOR_CHANNELLED"
        "MaxLevel"                      "1"

        "AbilityCooldown"               "0"
        "AbilityChannelTime"			"1.5"

        "OnSpellStart"
		{
			"FireSound"
			{
				"Target"				"UNIT"
				"EffectName"			"scene_voice_box_collect"
			}

			"ApplyModifier"
			{
				"Target"				"CASTER"
				"ModifierName"			"modifier_channel_act_datadriven"
			}
		}

        "OnChannelSucceeded"
        {			
            "FireSound"
            {
                "EffectName"		"scene_voice_coin_drop"
                "Target"			"UNIT"
            }           

            "RunScript"
            {
                "ScriptFile"		    "scene/order_click.lua"
                "Function"			    "initHeroOpenBoxChannelSucceeded"
            }
        }

        "OnChannelInterrupted"
		{
            "FireSound"
            {
                "EffectName"		"scene_voice_stop"
                "Target"			"UNIT"
            }  

            "RemoveModifier"
			{
				"Target"				"CASTER"
				"ModifierName"			"modifier_channel_act_datadriven"
			}
        }

        "Modifiers"
        {
            "modifier_channel_act_datadriven"
            {
                "Duration"				"1.5"
				"OverrideAnimation"     "ACT_DOTA_GENERIC_CHANNEL_1"
            }
        }
    }

